<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="622.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="DEFINE PLAYERS" />
      <HBox prefHeight="100.0" prefWidth="496.0">
         <children>
            <Label text="Player 1" />
            <ComboBox fx:id="tplayer1ComboBox" prefWidth="150.0" />
            <TextField fx:id="namePlayer1TextField" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Player 2" />
            <ComboBox fx:id="tplayer2ComboBox" prefWidth="150.0" />
            <TextField fx:id="namePlayer2TextField" />
         </children>
      </HBox>
      <Button fx:id="startButton" mnemonicParsing="false" text="START" />
   </children>
</VBox>
